mod std.io;

import (
    std
)

interface receive_channel<t> {
    def read() : t;

    def read(filter: int) : t;

    def read_next(filter: int, count: int) : list<t>;

    def read_next(filter: int) : list<t>;

    def read_all(filter: int) : list<t>;

    def next() : int;

    def avail_data(filter: int) : bool;
}
