interface receive_channel<t> {
    def read() : t;

    def read(filter: var) : t;

    def read_next(filter: var, count: var) : list<t>;

    def read_next(filter: var) : list<t>;

    def read_all(filter: var) : list<t>;

    def next() : var;

    def avail_data(filter: var) : var;
}
