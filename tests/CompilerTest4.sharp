class Callback {

    public def delegate::work();
}

interface Event {
    public def delegate::event();
}

class Callee base Callback : Event {

    public def delegate::event() {
        System.print("Event Called!");
    }

    public def delegate::work() {
        System.print("success I am Called!");
    }
}