mod main;

import platform.kernel;
import std.io;

def main() {
    arry := new var[10000000];
    past := time.mills_time(), now;
    val := 0;
    for(i := 0; i < sizeof(arry); i++) {
        val = arry[i];
    }
    now = time.mills_time();
    println(("for (" + (now-past) + ") ms\n").data);

    past = time.mills_time();
    foreach(n in arry) {
        val = n;
    }
    now = time.mills_time();
    println(("foreach (" + (now-past) + ") ms\n").data);
}
