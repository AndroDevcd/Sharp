def factorCount(n: var) : var {
    square: var = sqrt(n);
    isquare: var = square as _int64;
    count: var = isquare == square ? -1 : 0;
    while(isquare) {
        if(0 == n % isquare) count += 2;
        isquare--;
    }
    return count;
}