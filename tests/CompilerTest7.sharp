mod application;

import std;

public class Application {


    enum Enums {
        APPLE = 90999,
        CHERRY = 17,
        PEACH
    };

    public static def foo() : var {
        return 22;
    }

    public static def main(string[] args) : var {
         var data = (var)Enums.CHERRY;
        asm(" nop nop nop nop nop nop");
        switch(data) {
            case Enums.APPLE:
                System.print("apple");
                break;
            case Enums.CHERRY:
                System.print("cherry");
                break;
            case Enums.PEACH:
                System.print("peach");
                break;
            case 7:
                System.print("7");
                break;
            case (_int16)4.3:
                break;
            default:
                System.print("default");
                break;
        }
        var a = 1;
        var b = 2;
        var c = 3;

        // 1 + 2 * 3 +1*2 + 1
        System.println(a + b * c + a * b + a);
    }
}