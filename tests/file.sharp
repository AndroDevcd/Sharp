mod app;

import std;
import std.kernel;
import std.io;

def main() {
    File file = new File(new string("README1.md"));

    println("file " + file.getPath() + " being created  ");
    file.create();

    file.setWritable(true);
    IOStream ostream = new IOStream(file, IOStream.WRITE);
    ostream.write("Hello, World!\n");
    ostream.close();

    IOStream istream = new IOStream(file, IOStream.READ);
    println(istream.nextLine());
    istream.close();
}
