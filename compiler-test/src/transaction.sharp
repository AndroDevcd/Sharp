class transaction {
    amount: long;
    memo: string;
    payee: string;
    time: long;

    private transaction() {}

    transaction(
        amount: long,
        memo: string,
        payee: string,
        time: long
    ) {
        self->amount = amount;
        self->memo = memo;
        self->payee = payee;
        self->time = time;
    }

    override def to_string(): string {
        return "[at: $time, $amount, memo: $memo, to/from: $payee]";
    }
}
